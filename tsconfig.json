{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2015"],
    "outDir": "dist",
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "sourceMap": true,
    "inlineSources": true,
    "strictNullChecks": true,
    "types": ["node", "jest"]
  },
  "files": ["src/free-style.ts", "src/free-style.spec.ts"]
}
